{
    "name": "bittyphp/bitty-container",
    "description": "PSR-11 Container implementation.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Luke Kingsley",
            "email": "bizurkur@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.1.0",
        "psr/container": "^1.0",
        "container-interop/service-provider": "^0.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0",
        "squizlabs/php_codesniffer": "^3.0",
        "jakub-onderka/php-parallel-lint": "^1.0",
        "localheinz/composer-normalize": "^1.1"
    },
    "provide": {
        "psr/container-implementation": "1.0"
    },
    "autoload": {
        "psr-4": {
            "Bitty\\Container\\" : "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Bitty\\Tests\\Container\\": "tests/"
        }
    },
    "minimum-stability": "stable"
}
